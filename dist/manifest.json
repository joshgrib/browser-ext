{
  "manifest_version": 2,
  "name": "josh-chrome-extension",
  "version": "1.4.0",
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.bundle.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-3.4.1.slim.min.js",
        "content.bundle.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
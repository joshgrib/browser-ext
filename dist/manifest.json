{
  "manifest_version": 2,
  "name": "My Cool Extension",
  "version": "0.1",
  "background": {
    "scripts": ["background.bundle.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "jquery-3.4.1.slim.min.js",
        "content.bundle.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ]
}
